{
  "name": "GIFReplace",
  "description": "This extension will replace animated gifs on a webpage.",
  "version": "0.1.0",
  "background" : {
    "scripts":["background.js"],
    "persistent": false
  },
  "browser_action": {
   "default_title": "Replace GIFs",
   "default_icon": "icon.png"
  },
  "permissions": [
   "activeTab"
 ],
  "manifest_version": 2
}
